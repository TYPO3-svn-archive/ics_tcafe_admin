<?xml version="1.0" encoding="utf-8" standalone="yes" ?>
<T3DataStructure>
	<meta>
       <langDisable>1</langDisable>
	</meta>
	<sheets>
		<general>
			<ROOT>
				<TCEforms>
					<sheetTitle>LLL:EXT:ics_tcafe_admin/locallang_flexform_pi1.xml:general</sheetTitle>
				</TCEforms>
				<el>
					<what_to_display>
						<TCEforms>
							<label>LLL:EXT:ics_tcafe_admin/locallang_flexform_pi1.xml:what_to_display</label>
							<config>	
								<type>select</type>
								<items type="array">
								<numIndex index="0" type="array">
									<numIndex index="0">LIST</numIndex>
									<numIndex index="1">LIST</numIndex>
								</numIndex>
								<numIndex index="1" type="array">
									<numIndex index="0">SINGLE</numIndex>
									<numIndex index="1">SINGLE</numIndex>
								</numIndex>
								<numIndex index="2" type="array">
									<numIndex index="0">EDIT</numIndex>
									<numIndex index="1">EDIT</numIndex>
								</numIndex>
								<numIndex index="3" type="array">
									<numIndex index="0">NEW</numIndex>
									<numIndex index="1">NEW</numIndex>
								</numIndex>
								</items>
								<itemsProcFunc>tx_icstcafeadmin_dynflex->user_insertExtraCodes</itemsProcFunc>
								<maxitems>4</maxitems>
								<size>4</size>
								<multiple>0</multiple>
							</config>
						</TCEforms>
					</what_to_display>
					<templateFile>
						<TCEforms>
							<label>LLL:EXT:ics_tcafe_admin/locallang_flexform_pi1.xml:templateFile</label>
							<config type="array">
								<type>input</type>
								<size>30</size>
								<max>255</max>
								<eval>trim</eval>
								<wizards type="array">
									<_PADDING>2</_PADDING>
									<link type="array">
										<type>popup</type>
										<title>Link</title>
										<icon>link_popup.gif</icon>
										<script>browse_links.php?mode=wizard</script>
										<params type="array">
											<blindLinkOptions>page,url,mail,spec</blindLinkOptions>
											<allowedExtensions>htm,html,tmpl,tpl</allowedExtensions>
										</params>
										<JSopenParams>height=300,width=500,status=0,menubar=0,scrollbars=1</JSopenParams>
									</link>
								</wizards>
							</config>
						</TCEforms>
					</templateFile>
				</el>
			</ROOT>
		</general>
		<table>
			<ROOT>
				<TCEforms>
					<sheetTitle>LLL:EXT:ics_tcafe_admin/locallang_flexform_pi1.xml:table</sheetTitle>
				</TCEforms>
				<el>
					<tablename>
						<TCEforms>
							<label>LLL:EXT:ics_tcafe_admin/locallang_flexform_pi1.xml:tablename</label>
							<onChange>reload</onChange>
							<config>
								<type>input</type>
								<size>30</size>
								<max>255</max>
								<eval>trim</eval>
							</config>
						</TCEforms>
					</tablename>
					<!-- ###ADDITIONAL FLEX TABLEFIELDS### -->
					<withUid>
						<TCEforms>
							<label>LLL:EXT:ics_tcafe_admin/locallang_flexform_pi1.xml:withUid</label>
							<config>
								<type>check</type>
							</config>
						</TCEforms>
					</withUid>
				</el>
			</ROOT>
		</table>
		<miscellaneous>
			<ROOT>
				<TCEforms>
					<sheetTitle>LLL:EXT:ics_tcafe_admin/locallang_flexform_pi1.xml:miscellaneous</sheetTitle>
				</TCEforms>
				<el>
					<singleID>
						<TCEforms>
							<exclude>1</exclude>
							<label>LLL:EXT:ics_tcafe_admin/locallang_flexform_pi1.xml:singleID</label>
							<config>
							<type>group</type>
							<internal_type>db</internal_type>
							<allowed>pages</allowed>
							<size>1</size>
							<maxitems>1</maxitems>
							<minitems>0</minitems>
							<show_thumbs>1</show_thumbs>
							</config>
						</TCEforms>
					</singleID>
					<withSingleData>
						<TCEforms>
							<label>LLL:EXT:ics_tcafe_admin/locallang_flexform_pi1.xml:withSingleData</label>
							<config>
								<type>check</type>
							</config>
						</TCEforms>
					</withSingleData>
					<editID>
						<TCEforms>
							<exclude>1</exclude>
							<label>LLL:EXT:ics_tcafe_admin/locallang_flexform_pi1.xml:editID</label>
							<config>
							<type>group</type>
							<internal_type>db</internal_type>
							<allowed>pages</allowed>
							<size>1</size>
							<maxitems>1</maxitems>
							<minitems>0</minitems>
							<show_thumbs>1</show_thumbs>
							</config>
						</TCEforms>
					</editID>
					<withEditData>
						<TCEforms>
							<label>LLL:EXT:ics_tcafe_admin/locallang_flexform_pi1.xml:withEditData</label>
							<config>
								<type>check</type>
							</config>
						</TCEforms>
					</withEditData>
					<newID>
						<TCEforms>
							<exclude>1</exclude>
							<label>LLL:EXT:ics_tcafe_admin/locallang_flexform_pi1.xml:newID</label>
							<config>
							<type>group</type>
							<internal_type>db</internal_type>
							<allowed>pages</allowed>
							<size>1</size>
							<maxitems>1</maxitems>
							<minitems>0</minitems>
							<show_thumbs>1</show_thumbs>
							</config>
						</TCEforms>
					</newID>
					<withNewData>
						<TCEforms>
							<label>LLL:EXT:ics_tcafe_admin/locallang_flexform_pi1.xml:withNewData</label>
							<config>
								<type>check</type>
							</config>
						</TCEforms>
					</withNewData>
					<!--<return>
						<TCEforms>
							<exclude>1</exclude>
							<label>LLL:EXT:ics_tcafe_admin/locallang_flexform_pi1.xml:returnUrl</label>
							<config>
							<type>group</type>
							<internal_type>db</internal_type>
							<allowed>pages</allowed>
							<size>1</size>
							<maxitems>1</maxitems>
							<minitems>0</minitems>
							<show_thumbs>1</show_thumbs>
							</config>
						</TCEforms>
					</return>-->
				</el>
			</ROOT>
		</miscellaneous>
	</sheets>
</T3DataStructure>